#include<iostream>
#include<string.h>
using namespace std;

class Mother 
{
    public:
    void display()
    {
      cout<<"Im from Mother class \n";
    }

};


class Daughter : public Mother
{
    public:
    void display()
    {
      cout<<"Im from Daughter class";
    }

};

int main() {
    Daughter d;

     d.display();

}
